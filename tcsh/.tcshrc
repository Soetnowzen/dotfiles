#!/bin/tcsh

set autoexpand

alias ls ls -F --color --group-directories-first
alias la ls -A
alias ll la -l
alias :q exit
alias find find \!* | grep '[^/]*$'
alias g git
alias grep grep --color
alias grepc grep -Rin --color --include=*.{cc,h}
alias less less -r
alias print_path 'echo $PATH | tr : "\n"'
alias save_pl git stash && git pull && git stash pop
alias tm 'tmux attach || tmux new'
alias v vim
alias vimr vim ~/.vimrc

setenv TERM xterm-256color

alias test 'sed -e "s/[a-zA-Z\_\.]\+\:[0-9]\+/%{\033[1;31m%} & %{\033[0m%}/g"'
alias color_failure grep -i \'err\\w*\\\|warn\\w*\\\|info\\w*\\\|fail\\w*\\\|\$\'

alias getCurrentGitBranch 'git branch |& grep \* | sed "s/\* \(.*\)/ (\1)/"'
alias precmd set 'prompt = "%{\033[36m%}%T %{\033[34m%}%n@%m %{\033[32m%}%~%{\033[33m%}`getCurrentGitBranch`%{\033[0m%}%# "'

# bindkey -v

# Autocomple searches when using up and down
bindkey "^[[A" history-search-backward
bindkey "^[[B" history-search-forward

# Ericsson (Don't push this)
alias kp5_setup '/repo/eseblag/ncmain/utils/kp5_setup'

# module add clang
module add cppcheck
module add ctags
module add lynx
module add meld
module add pygtk
module add tmux
