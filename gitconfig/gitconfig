[core]
  editor = vim
[color]
  ui = auto
[alias]
  st = status
  a = add
  ps = push
  pl = pull
  f = fetch
  c = commit
  lg1 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
  lg2 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
  lg = !"git lg6"
  lg3 = log --oneline --graph --decorate --all
  lg4 = log --oneline --graph --color --all --decorate
  lg5 = log --graph --abbrev-commit --decorate --format=format:'%C(bold red)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim green)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
  lg6 = log --pretty=format:"%C(Yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --graph
